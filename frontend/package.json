{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:coverage": "vitest run --coverage --watch=false",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "lint": "eslint ./src",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
